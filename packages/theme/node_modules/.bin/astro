#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/yianwark/projectTest-yian/public/node_modules/.pnpm/astro@4.16.19_@types+node@24.3.1_rollup@4.50.1_typescript@5.9.2/node_modules/astro/node_modules:/mnt/d/yianwark/projectTest-yian/public/node_modules/.pnpm/astro@4.16.19_@types+node@24.3.1_rollup@4.50.1_typescript@5.9.2/node_modules:/mnt/d/yianwark/projectTest-yian/public/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/yianwark/projectTest-yian/public/node_modules/.pnpm/astro@4.16.19_@types+node@24.3.1_rollup@4.50.1_typescript@5.9.2/node_modules/astro/node_modules:/mnt/d/yianwark/projectTest-yian/public/node_modules/.pnpm/astro@4.16.19_@types+node@24.3.1_rollup@4.50.1_typescript@5.9.2/node_modules:/mnt/d/yianwark/projectTest-yian/public/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
