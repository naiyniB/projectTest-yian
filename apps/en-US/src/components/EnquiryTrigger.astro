---
import { ModalTrigger } from "@rxdrag/website-lib";
interface Props {
  class?: string;
  callToAction?: string;
}
const { class: className, callToAction } = Astro.props;---

<ModalTrigger openableKey="enquiry-modal" class:list={["cursor-pointer btn btn-primary", className]} callToAction={callToAction}>
  <slot />
</ModalTrigger>